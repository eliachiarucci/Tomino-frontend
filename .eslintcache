ata) => {\n        history.push(\"/login\");\n      })\n      .catch((err) => {\n        const { message } = err.response.data;\n        setErrorMessage(message);\n      });\n  };\n\n  const uploadUrl = () => {\n    return env.SERVER_URL + \"/upload\";\n  };\n\n  return (\n    <FlexContainer type=\"vertical\" alignItems=\"center\" justifyContent=\"center\" width=\"100%\" height=\"50%\">\n      <Title>Sign Up:</Title>\n      <Form name=\"signup\" className=\"auth_form\" initialValues={{ remember: true }} onFinish={onFinish}>\n        <Form.Item name=\"username\" rules={[{ required: true, message: \"Please input your Username!\" }]}>\n          <Input prefix={<UserOutlined className=\"site-form-item-icon\" />} placeholder=\"Username\" />\n        </Form.Item>\n        <Form.Item name=\"email\" rules={[{ type: \"email\", required: true, message: \"Please input your Email!\" }]}>\n          <Input prefix={<MailOutlined className=\"site-form-item-icon\" />} type=\"email\" placeholder=\"Email\" />\n        </Form.Item>\n        <Form.Item name=\"password\" rules={[{ required: true, message: \"Please input your Password!\" }]}>\n          <Input prefix={<LockOutlined className=\"site-form-item-icon\" />} type=\"password\" placeholder=\"Password\" />\n        </Form.Item>\n        <Form.Item\n          name=\"image\"\n          label=\"Image\"\n          valuePropName=\"file\"\n          getValueFromEvent={normFile}\n          rules={[{ required: true, message: \"Please upload a profile picture!\" }]}\n        >\n          <Upload accept=\".jpg, .jpeg, .png\" multiple={false} name=\"image\" action={uploadUrl} listType=\"picture\">\n            <Button icon={<UploadOutlined />}>Click to upload</Button>\n          </Upload>\n        </Form.Item>\n        <Form.Item>\n          <Button type=\"primary\" htmlType=\"submit\" className=\"form-button\">\n            Sign Up\n          </Button>\n          <br></br>\n          If you already have an account, <a href=\"\">Login</a>\n        </Form.Item>\n        <Text type=\"danger\">{errorMessage}</Text>\n      </Form>\n    </FlexContainer>\n  );\n};\n\nexport default Signup;\n","/Users/eliachiarucci/git/mr-pan-frontend/src/screens/user/NewRecipe.tsx",[],"/Users/eliachiarucci/git/mr-pan-frontend/src/services/recipe-service.js",[],"/Users/eliachiarucci/git/mr-pan-frontend/src/screens/user/Recipe.tsx",["80","81","82"],"/Users/eliachiarucci/git/mr-pan-frontend/src/components/Timer/Timer.tsx",["83","84"],"import React, { useEffect, useState } from \"react\";\n\ninterface props {\n  time: number;\n  tickRate?: number;\n  running?: boolean;\n  onTick?: Function;\n  onFinish?: Function;\n  name?: string | number;\n}\n\nconst Timer = ({ time, tickRate = 1000, running = true, onTick, onFinish, name }: props) => {\n  const [timer, setTimer] = useState<number>(time);\n  const [intervalState, setIntervalState] = useState<any>();\n\n  useEffect(() => {\n    clearInterval(intervalState);\n    setIntervalState(\n      setInterval(() => {\n        if (running) {\n          setTimer((oldTimer) => oldTimer - 1);\n        }\n      }, tickRate)\n    );\n    console.log(running);\n  }, [running]);\n\n  useEffect(() => {\n    if (timer < 1) {\n      clearInterval(intervalState);\n      if (onFinish) onFinish();\n    }\n\n    if (onTick) onTick(timer, name);\n  }, [timer]);\n\n  return <div>{timer}</div>;\n};\n\nexport default Timer;\n",{"ruleId":"85","severity":1,"message":"86","line":2,"column":23,"nodeType":"87","messageId":"88","endLine":2,"endColumn":31},{"ruleId":"85","severity":1,"message":"89","line":2,"column":41,"nodeType":"87","messageId":"88","endLine":2,"endColumn":47},{"ruleId":"85","severity":1,"message":"90","line":3,"column":38,"nodeType":"87","messageId":"88","endLine":3,"endColumn":52},{"ruleId":"85","severity":1,"message":"91","line":4,"column":8,"nodeType":"87","messageId":"88","endLine":4,"endColumn":13},{"ruleId":"92","severity":1,"message":"93","line":46,"column":14,"nodeType":"94","endLine":